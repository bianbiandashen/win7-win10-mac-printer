{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "http://127.0.0.1:1388",
    "distDir": "../webui/public"
  },
  "package": {
    "productName": "xhs-electron-printer",
    "version": "0.0.1"
  },
  "tauri": {
    "windows": [
      {
        "title": "小红书电子面单打印",
        "width": 1200,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    },
    "allowlist": {
      "all": false,
      "fs": {
        "all": true
      },
      "http": {
        "all": true
      },
      "window": {
        "all": true
      },
      "shell": {
        "all": true
      }
    },
    "bundle": {
      "resources": [
        "binaries/bin"
      ],
      "active": true,
      "targets": [
        "dmg",
        "msi",
        "nsis",
        "updater"
      ],
      "identifier": "com.example.tauri-plugin-printer",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "macOS": {
        "frameworks": [],
        "license": "MIT",
        "signingIdentity": "-"
      },
      "windows": {
        "webviewInstallMode": {
          "type": "embedBootstrapper",
          "silent": true
        }
      }
    },
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": [
        "https://pc-infra.devops.sit.xiaohongshu.com/api/pc-infra/release?applicationId=electronic-print&localVersion=0.0.1&platform=win32&arch=x64&appType=tauri&flavor=squirrel"
